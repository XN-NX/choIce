    "def hellos():\n",
    "    yield 'hi'\n",
    "    yield 'hello'\n",
    "    yield 'whats up'\n",
    "\n",
    "\n",
    "hello_object = hellos()\n",
    "\n",
    "\n",
    "print('entering loop')\n",
    "for phrase in hello_object:\n",
    "    print(phrase)\n",
    "print('left loop')  # loops will run until StopIteration is raised"
   ]
