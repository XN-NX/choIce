    "# lazy evaluation!\n",
    "def gen():\n",
    "    print('Working on item 1')\n",
    "    yield 'item 1'  # Stop! Do work on second thing yet!\n",
    "    \n",
    "    print('Working on item 2')\n",
    "    yield 'item 2'\n",
    "\n",
    "mygen = gen()\n",
    "\n",
    "print(next(mygen))\n",
    "# do other things without working on item 2"
   ]
