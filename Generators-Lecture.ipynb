    "#iterate generator within generator\n",
    "def gen(x):\n",
    "    other_gen = range(x)\n",
    "    for i in other_gen:\n",
    "        yield i\n",
    "\n",
    "for i in gen(5):\n",
    "    print('From verbose method:', i)\n",
    "\n",
    "print()\n",
    "\n",
    "def gen(x):\n",
    "    yield from range(x)  # yield from allows us to pull from another iterable\n",
    "\n",
    "for i in gen(5):\n",
    "    print('From yield from:', i)"
   ]
